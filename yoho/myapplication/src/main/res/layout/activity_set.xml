<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".mvp.ui.activity.SetActivity">
    <RelativeLayout
        android:id="@+id/ll"
        android:background="#615F5F"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:id="@+id/set_back_activity"
            android:layout_height="wrap_content"
            android:src="@drawable/back_icon"></ImageView>
        <TextView
            android:layout_marginLeft="40dp"
            android:layout_centerVertical="true"
            android:textColor="#FFFFFF"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13sp"
            android:textSize="15sp"
            android:text="设置"></TextView>
    </RelativeLayout>
    <Button
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="退出登录"
        android:layout_alignParentBottom="true"
        android:background="#e12"
        android:id="@+id/set_tuichu_activity"
        android:textSize="20sp"
        android:textColor="#FFFFFF"></Button>
<ScrollView
    android:layout_below="@id/ll"
    android:layout_above="@+id/set_tuichu_activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="个人信息"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/community_msg_center_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"></ImageView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="地址管理"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/community_msg_center_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"></ImageView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="账号安全"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/community_msg_center_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"></ImageView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="隐私设置"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/community_msg_center_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"></ImageView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="账号安全"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:text="已开启"
                android:textSize="15sp"
                android:layout_centerVertical="true"></TextView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2G/3G网络显示原图"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ToggleButton
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/toggle_selector"
                android:textOn=""
                android:textOff=""
                android:checked="true"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="wifi环境下自动更新"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ToggleButton
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/toggle_selector"
                android:textOn=""
                android:textOff=""
                android:checked="true"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="摇一摇,截屏反馈Bug"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ToggleButton
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:background="@drawable/toggle_selector"
                android:textOn=""
                android:textOff=""
                android:checked="true"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="清除缓存"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:text="14.92M"
                android:textSize="15sp"
                android:layout_centerVertical="true"></TextView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="评价有货"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/community_msg_center_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"></ImageView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="关于有货"
                android:textSize="15sp"

                android:textColor="#090909"></TextView>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/community_msg_center_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"></ImageView>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="#E0DEDF"
                android:layout_alignParentBottom="true"
                ></TextView>
        </RelativeLayout>
        <TextView
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="更多应用"
            android:textSize="15sp"
            android:textColor="#090909"></TextView>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@mipmap/yn"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="80dp"></ImageView>
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:src="@mipmap/mm"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="50dp"></ImageView>
        </LinearLayout>
    </LinearLayout>
</ScrollView>

</RelativeLayout>